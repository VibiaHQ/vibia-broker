<!-- create a .plist file on ~/Library/LaunchAgents -->
<!-- nano ~/Library/LaunchAgents/protofy.startproject.plist -->
<!-- and copy this -->
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.protofy.startproject</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/Protofy/apps/launcher/mac/index.sh</string> <!-- Path of your script -->
    </array>
    <key>RunAtLoad</key>
    <true/>
</dict>
</plist>